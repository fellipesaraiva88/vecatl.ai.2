{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "DATABASE_URL": "file:./dev.db"
  },
  "build": {
    "env": {
      "DATABASE_URL": "file:./dev.db"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
